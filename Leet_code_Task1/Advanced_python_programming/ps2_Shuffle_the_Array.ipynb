    "class Solution:\n",
    "    def shuffle(self, nums: List[int], n: int) -> List[int]:\n",
    "        final_list=[]\n",
    "        for i in range(len(nums)//2):\n",
    "            final_list.append(nums[i])\n",
    "            final_list.append(nums[(len(nums)//2)+i])\n",
    "        return(final_list)"
   ]
