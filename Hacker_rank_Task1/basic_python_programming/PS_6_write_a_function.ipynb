   "source": [
    "def is_leap(year):\n",
    "    if year%400==0:\n",
    "        return True\n",
    "    if (year%4==0) and (year%100==0):\n",
    "        return False\n",
    "    if year%4==0:\n",
    "        return True\n",
    "    return False\n",
    "\n",
    "\n",
    "year = int(input())\n",
    "print(is_leap(year))"
   ]
